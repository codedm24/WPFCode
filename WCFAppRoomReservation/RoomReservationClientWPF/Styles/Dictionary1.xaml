<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:model="clr-namespace:RoomReservationClientWPF.RoomReservationService">
    <DataTemplate x:Key="RoomReservationTemplate" DataType="model:RoomReservation">
        <StackPanel Orientation="Horizontal" Width="Auto">
            <TextBlock Text="StartDate" Width="80"/>
            <TextBlock Text="{Binding StartDate.Date}" Width="80" />
            <TextBlock Text="EndDate"  Width="80" />
            <TextBlock Text="{Binding EndDate.Date}"  Width="80" />
            <TextBlock Text="Contact" Width="80" />
            <TextBlock Text="{Binding Contact}"  Width="80" />
            <TextBlock Text="Event"  Width="80" />
            <TextBlock Text="{Binding Event}"  Width="80" />
            <TextBlock Text="RoomName"  Width="80" />
            <TextBlock Text="{Binding RoomName}"  Width="80" />
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>